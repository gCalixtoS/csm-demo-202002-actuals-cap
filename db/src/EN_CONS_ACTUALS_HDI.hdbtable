COLUMN TABLE "EN_CONS_ACTUALS_HDI"(
	"ID"				INTEGER 	NOT NULL,
	"YEAR"				INTEGER,
	"MONTH" 		    NVARCHAR(2),
	"WEEKNUM" 			NVARCHAR(2),
	"WEEKDAY" 		    INTEGER,
	"DAY"	            NVARCHAR(2),
	"WEEKEND_YN"		NVARCHAR(1),
	"HOUR"			    NVARCHAR(2),
	"MINUTE"			NVARCHAR(2),
	"RECORDMODE"		NVARCHAR(5000),
	"UNIT"			    NVARCHAR(3),
	PRIMARY KEY ("ID")
) COMMENT 'Energy consumption information loaded from data lake or BW on HANA' COLUMN LOADABLE UNLOAD PRIORITY 0;