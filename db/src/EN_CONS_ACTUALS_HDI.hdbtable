COLUMN TABLE "EN_CONS_ACTUALS_HDI"(
	"YEAR_"				INTEGER,
	"MONTH_" 		    NVARCHAR(2),
	"WEEKNUM_" 			NVARCHAR(2),
	"WEEKDAY_" 		    INTEGER,
	"DAY_"	            NVARCHAR(2),
	"WEEKEND_YN_"		NVARCHAR(1),
	"HOUR_"			    NVARCHAR(2),
	"MINUTE_"			NVARCHAR(2),
    "RECORDMODE"		NVARCHAR(5000),
    "EN_CONS_ACTUAL"    DECIMAL(17, 0),
    "UNIT"			    NVARCHAR(3)
) COMMENT 'Energy consumption information loaded from data lake or BW on HANA' COLUMN LOADABLE UNLOAD PRIORITY 0;